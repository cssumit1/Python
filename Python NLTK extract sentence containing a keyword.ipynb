   "source": [
    "searchFile = open('keywords.txt','r')\n",
    "file = open('corpodssqlprd01_cluster.log','r') \n",
    "file1 = file.readlines()  \n",
    "file.close()\n",
    "for key in searchFile:\n",
    "    for line in file1:\n",
    "        if key in line:\n",
    "             print (line)"
